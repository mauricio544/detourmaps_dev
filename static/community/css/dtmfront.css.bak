    margin: 5px !important;
    background: url(../img/save_detour.svg) no-repeat !important;
}
.tab_video_mail{
    float:left; display:inline-block; width: 45px !important; height: 45px !important;
    margin: 5px !important;
    background: url(../img/share_detour.svg) no-repeat !important;
    background-position: 5px 5px;
}
.tab_video_card{
    float:left; display:inline-block; width: 45px !important; height: 45px !important;
    margin: 5px !important;
    background: url(../img/savi10_detour.svg) no-repeat !important;
    background-position: 5px 5px;
}

.tab_review{
    height: 212px;
    padding: 2px;
}

#tab_review_comment{
    height: 172px;
    margin-top: 5px;
}

.tab_review_btn {
    width: 100px;
    height: 24px;
    padding: 2px 8px 2px 8px;
    margin: 0px 2px;
    color: #fff;
    font-weight: bold;
    cursor: pointer;
    font-family: 'Istok Web', sans-serif;
    background: #0A90C0;
    background: -moz-linear-gradient(top, #addff8 0%, #3399ff 100%);
    background: -webkit-linear-gradient(top, #addff8 0%,#3399ff 100%);
    background: -o-linear-gradient(top, #addff8 0%,#3399ff 100%);
    background: -ms-linear-gradient(top, #addff8 0%,#3399ff 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#addff8', endColorstr='#3399ff',GradientType=0 );
    border: 1px white solid;
    border-radius: 0px;
    -moz-border-radius: 0px;
    -webkit-border-radius: 0px;
}
.review_img{
    position: relative;
    overflow: hidden;
}
.review_img img{
    max-height: 150px;
}

.social_btn{
    display: inline-block;
    margin-top: 60px;
    margin-right: 2px;
}
.icon_facebook{
    background-image: url(/static/community/img/icons/www-sharing-vflIBlxAE.png) !important;
    background-position: 0px -50px;
    background-repeat: no-repeat;
    width: 32px !important;
    height: 32px !important;
    vertical-align: middle;
    display: inline-block;
    cursor: pointer;
}
.icon_twitter{
    background-image: url(/static/community/img/icons/www-sharing-vflIBlxAE.png) !important;
    background-position: 0px -1253px;
    background-repeat: no-repeat;
    width: 32px !important;
    height: 32px !important;
    vertical-align: middle;
    display: inline-block;
    cursor: pointer;
}
.icon_email{
    background-image: url(../img/email.png) !important;
    background-repeat: no-repeat;
    width: 32px !important;
    height: 32px !important;
    vertical-align: middle;
    display: inline-block;
    cursor: pointer;
}

#overSavings{
    background-image: url(../img/deals-box.jpg) !important;
    background-repeat: no-repeat;
    overflow: hidden;
}
#overSavings_30{
    position: absolute;
    width: 180px;
    height: 290px;
    top: 110px;
    left: 45px;
    background-color: rgba(255, 222, 0, 0.2);
    overflow-y: auto;
}
#overSavings_40{
    position: absolute;
    width: 180px;
    height: 290px;
    top: 110px;
    left: 245px;
    background-color: rgba(255, 0, 0, 0.2);
    overflow-y: auto;
}
#overSavings_50{
    position: absolute;
    width: 180px;
    height: 290px;
    top: 110px;
    left: 445px;
    background-color: rgba(0, 200, 255, 0.2);
    overflow-y: auto;
}
#overSavings_30 li,
#overSavings_40 li,
#overSavings_50 li{
    cursor: pointer;
}
#overSavings_30 li:hover,
#overSavings_40 li:hover,
#overSavings_50 li:hover{
    background-color: rgba(255, 100, 0, 0.6);
}

#overDiscover div{
    position: absolute;
    top: 0;
    left: 0;
    display: none;
}
#overDiscover span{
    position: absolute;
    cursor: pointer;
    opacity:0.4;
    filter:alpha(opacity=40);
}
#overDiscover span:hover{
    opacity:1;
    filter:alpha(opacity=100);
}
#overDiscover span._left{
    top:220px;
    left: 0;
}
#overDiscover span._right{
    top:220px;
    right: 0;
}

#userLogin{
    background-image: url(../img/sign-in-bg.jpg);
    background-repeat: no-repeat;
    background-position: -90px -145px;
}
#printedMaps{
    background-color: white;
    background-image: url(../img/sign-in-bg.jpg);
    background-repeat: no-repeat;
    background-position: -30px 35px;
}

#events_box{
    position: absolute;
    width: 710px;
    height: 503px;
    padding: 20px;
    background-image: url(../img/events-box.jpg);
    background-repeat: no-repeat;
}
#events_box section{
    width: 510px;
    height:268px;
    position: absolute;
    top:125px;
    left:122px;
    overflow-x:hidden;
    overflow-y: auto;
}
#events_box li{
    cursor: pointer;
    color: white;
    font-size: 1.2em;
    margin-bottom: 2px;
}

.snap_slider:hover{
    position: absolute;
    cursor: pointer;
    opacity: 1;
    filter: alpha(opacity=100);
}
.snap_slider{
    position: absolute;
    cursor: pointer;
    opacity: 0.4;
    filter: alpha(opacity=40);
}

.comment{

}
.comment:hover{
    background-color: #eaf2fb;
}

.frame{
    width: 600px;
    height: 400px;
}

#bus_events_list{
    background-image: url(../img/deals-box.jpg);
    background-repeat: no-repeat;
    background-position: -121px -101px;
}

.ul_events_list li{
    color: #a9a9a9;
    list-style: none;
    text-align: left;
}

.onred:hover{
    color: #CC3300 !important;
    cursor: pointer !important;
}
