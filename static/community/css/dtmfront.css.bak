/* CSS Resets */
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,address,cite,code,del,dfn,em,img,ins,q,small,strong,sub,sup,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{border:0;margin:0;padding:0}article,aside,figure,figure img,figcaption,hgroup,footer,header,nav,section,video,object{display:block}a img{border:0}figure{position:relative}figure img{width:100%}

body {
    overflow: hidden;
    background: #DFE9F2;
    font-family: 'Istok Web', sans-serif;
    font-weight: 400;
    font-size: 0.9em;
    color: #333;
}
h1, h2, h3, h4, h5, h6 {
    font-family: 'Arvo', 'Georgia', serif;
    font-weight: 700;
    margin: 1em 0 0.8em 0;
}
p {
    margin: 0 0 10px 0;
}
a {
    color: #0A90C0;
    text-decoration: none;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    transition: all 0.3s ease-in-out;
    border-bottom: 1px rgba(10, 143, 191, 0) dashed;
}
/* Handy Generals */
.center {
    text-align: center;
}
#templates {
    display: none;
}
#map {
    position: relative;
    width: 100%;
    overflow: hidden;
}
/* Forms */
div.form {
    box-sizing: border-box;
    padding: 0 30px;
    border: 1px #ddd solid;
    margin: 5px 0;
    background: #fff;
}
div.form h2 {
    color: #F48131;
    padding-bottom: 0.2em;
    margin-bottom: 0.8em;
    border-bottom: 1px #CCC solid;
    font-size: 1.6em;
    font-weight: 700;
}
form p {
    margin-bottom: 15px;
}
form p.submit {
    text-align: right;
}
form p img {
    vertical-align: middle;
}
form label {
    margin: 0 20px 0 0;
    min-width: 150px;
    text-align: right;
}
form label.hidden {
    display: none;
    visibility: hidden;
}
form label.error {
    min-width: 0;
    color: #ff4500;
    font-size: 0.8em;
}
form input:not([type="checkbox"]), textarea, select {
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    font-family: 'Istok Web', sans-serif;
    width: 100%;
    padding: 6px 12px 4px;
    background: #F2f4F6;
    outline: none;
    color: #666;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border: 1px solid #ccc;
    border-radius: 2px;
    box-shadow: inset 0 0 1px rgba(10, 143, 191, 0);
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    transition: all 0.3s ease-in-out;
}
form input:hover, form select:hover, textarea:hover {
    background: #f6f8fa;
    border: 1px solid #aaa;
    box-shadow: inset 0 0 1px rgba(10, 143, 191, 0.4);
}
form input:focus, textarea:focus, select:focus {
    background: #fff;
    border: 1px solid #a2a6aa;
    box-shadow: inset 0 0 10px rgba(10, 10, 100, 0.2);
}
form input.error, form textarea.error, form select.error {
    border: 1px solid #ff4500;
}
form button, button, .btn {
    width: 132px;
    padding: 8px 16px 6px;
    margin: 2px 0;
    color: #fff;
    cursor: pointer;
    font-family: 'Istok Web', sans-serif;
    background: #f48131;
    background: -moz-linear-gradient(top, #ffaa33 0%, #ff8833 100%);
    background: -webkit-linear-gradient(top, #ffaa33 0%,#ff8833 100%);
    background: -o-linear-gradient(top, #ffaa33 0%,#ff8833 100%);
    background: -ms-linear-gradient(top, #ffaa33 0%,#ff8833 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffaa33', endColorstr='#ff8833',GradientType=0 );
    border: 1px #f48131 solid;
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
}
form button:not(.disabled):active, .btn:not(.disabled):active {
    background: #ff8833;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  #ff8833 0%, #ffaa33 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ff8833), color-stop(100%,#ffaa33));
    background: -webkit-linear-gradient(top,  #ff8833 0%,#ffaa33 100%);
    background: -o-linear-gradient(top,  #ff8833 0%,#ffaa33 100%);
    background: -ms-linear-gradient(top,  #ff8833 0%,#ffaa33 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ff8833', endColorstr='#ffaa33',GradientType=0 );
}
form button.disabled {
    opacity: 0.5;
}
/* Twelve col grid */
.container {
    padding-left: 20px;
    padding-right: 20px;
}
.row {
    width: 100%;
    max-width: 1140px;
    min-width: 755px;
    margin: 0 auto;
    overflow: hidden;
}
.onecol, .twocol, .threecol, .fourcol, .fivecol, .sixcol, .sevencol, .eightcol, .ninecol, .tencol, .elevencol {
    margin-right: 3.8%;
    float: left;
    min-height: 1px;
}
.row .onecol {
    width: 4.85%;
}
.row .twocol {
    width: 13.45%;
}
.row .threecol {
    width: 22.05%;
}
.row .fourcol {
    width: 30.75%;
}
.row .fivecol {
    width: 39.45%;
}
.row .sixcol {
    width: 48%;
}
.row .sevencol {
    width: 56.75%;
}
.row .eightcol {
    width: 65.4%;
}
.row .ninecol {
    width: 74.05%;
}
.row .tencol {
    width: 82.7%;
}
.row .elevencol {
    width: 91.35%;
}
.row .twelvecol {
    width: 100%;
    float: left;
}
.last {
    margin-right: 0px;
}
img, object, embed {
    max-width: 100%;
}
img {
    height: auto;
}
/* layout barras superior lateral derecha */
#map .tbar {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
    width: 100%;
    height: 60px;
    padding: 0 12px;
    overflow: hidden;
    color: #40607F;
    background:  #f5f6f7 url('../img/fondoDetour.jpg');
    box-shadow: 0 0 3px rgba(0,0,0,0.33);
    border-bottom: 1px #bababa solid;
}
#map .radius-wrap{
    margin: 0;
    overflow: hidden;
    padding: 12px;
    background-color: #DFE9F2;
}
#map .lbar {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
    float: left;
    height: 548px;
    width: 24%;
    margin-right: 0.5%;
    border: 1px solid #bababa;
    background: #fff none no-repeat 35px 55px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    -moz-transition: all 0.33s ease-out;
    -o-transition: all 0.33s ease-out;
    -webkit-transition: all 0.33s ease-out 0s;
    -ms-transition: all 0.33s ease-out;
    transition: all 0.33s ease-out;
}
#map .lbar.clear {
    background: #40607F no-repeat center 55px;
    border: 1px solid #DFE9F2;
}
#map .rbar {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin-right: 0;
    float: left;
    min-height: 1px;
    position: relative;
    vertical-align: top;
    display: inline-block;
    height: 550px;
    width: 75.5%;
    overflow: hidden;
}
#map .rbar img {
    max-width: inherit;
}
/* barra superior */
.tbar .logo {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    display: block;
    float: left;
    width: 24%;
    padding: 6px 20px;
}
.tbar .logo img {
    display: block;
    margin: 0 auto;
}
.tbar .tmenu {
    position: relative;
    display: block;
    float: left;
    border-left: 1px #bababa solid;
    border-right: 1px #bababa solid;
    padding: 0 40px 0 20px;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,0)));
    background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 );
    cursor: pointer;
}
.tbar .tmenu:after {
    position: absolute;
    content: '';
    height: 0;
    width: 0;
    top: 28px;
    right: 10px;
    border: 9px transparent solid;
    border-top-color: #40607F;
}
#detourmaps div{
    overflow: hidden;
}
.tbar .tmenu.detourmaps {
    float: right;
    border-left: 1px transparent solid;
    border-right: 1px transparent solid;
    padding: 0 20px;
    background: none;
    filter: none;
}
.tbar .tmenu.detourmaps:hover {
    border-left: 1px #bababa solid;
    border-right: 1px #bababa solid;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,0)));
    background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 );
}
.tbar .tmenu.detourmaps:after {
    position: absolute;
    content: '';
    height: 0;
    width: 0;
    top: 28px;
    right: 10px;
    border: 9px transparent solid;
    border-top-color: #40607F;
}
.tbar .tmenu.active, .tbar .tmenu.active:hover {
    border-left: 1px #bababa solid;
    border-right: 1px #bababa solid;
    background: #f5f6f7 url('../img/fondoDetour.jpg');
    z-index: 1;
}
.tbar .tmenu h1 {
    display: inline-block;
    margin: 9px 0;
    font-family: 'Istok Web', sans-serif;
    font-size: 2em;
    font-weight: 700;
    text-align: center;
    line-height: 1.5em;
    color: #40607F;
    text-shadow: 0 -1px 1px rgba(0, 0, 0, 0.5);
}
.tbar .tmenu .menu {
    display: none;
}

.tbar nav {
    display: block;
    float: left;
}
.tbar nav ul {
    display: block;
    list-style: none;
    text-align: center;
    margin-left: 20px;
}
.tbar nav ul li {
    display: block;
    text-align: center;
    float: left;
}
.tbar nav ul li a {
    position: relative;
    display: block;
    margin: 14px 2px 0;
    padding: 14px 16px 12px;
    font-size: 1.2em;
    font-weight: 400;
    text-align: center;
    color: #40607F;
    text-decoration: none;
    border: 1px solid transparent;
    border-bottom: none;
    -webkit-border-top-left-radius: 7px;
    -webkit-border-top-right-radius: 7px;
    -moz-border-radius-topleft: 7px;
    -moz-border-radius-topright: 7px;
    border-top-left-radius: 7px;
    border-top-right-radius: 7px;
    -moz-transition: all 0.33s ease-out;
    -o-transition: all 0.33s ease-out;
    -webkit-transition: all 0.33s ease-out 0s;
    -ms-transition: all 0.33s ease-out;
    transition: all 0.33s ease-out;
    z-index: 5;
    cursor: default;
}
.tbar nav ul li a.hover {
    color: #444;
    background-color: rgba(255, 255, 255, 1);
    border-color: #bababa;
}
.tbar nav ul.submenu {
    display: none;
}
.over {
    display: none;
    position: absolute;
    top: 58px;
    left: 160px;
    height: 450px;
    width: 640px;
    padding: 20px;
    box-shadow: 0 4px 12px 1px rgba(0, 0, 0, 0.35);
    border: 1px solid #bababa;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,0.8) 33%, rgba(255,255,255,0.8) 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(33%,rgba(255,255,255,0.8)), color-stop(100%,rgba(255,255,255,0.8)));
    background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.8) 33%,rgba(255,255,255,0.8) 100%);
    background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.8) 33%,rgba(255,255,255,0.8) 100%);
    background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.8) 33%,rgba(255,255,255,0.8) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ccffffff',GradientType=0 );
    z-index: 4;
}
.over.display {
    display: block;
}
.overmenu {
    display: inline-block;
    position: absolute;
    top: 58px;
    left: 30px;
    list-style: none;
    padding: 0;
    margin: 0;
    border: 1px #bababa solid;
    box-shadow: 0 4px 12px 1px rgba(0, 0, 0, 0.35);
    background:  #f5f6f7 url('../img/fondoDetour.jpg');
}
.overmenu li {
    display: block;
}
.overmenu a {
    display: block;
    padding: 7px 20px;
    font-size: 1.2em;
    font-weight: 700;
    text-decoration: none;
    color: #40607F;
}
.overmenu a:hover {
    background-color: #fff;
}
.over h1 {
    font-family: 'Arvo', Georgia, Utopia, Palatino, 'Palatino Linotype', serif;
    border-bottom: 1px solid #f48131;
    color: #f48131;
}
/* Google Map */
#detourmap {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    float: left;
    margin-top: 5px;
    height: 488px;
    width: 100%;
    border: 1px solid #bababa;
    -webkit-border-bottom-right-radius: 7px;
    -moz-border-radius-bottomright: 7px;
    border-bottom-right-radius: 7px;
    background-color: #fff;
}
#detourmap h3.cmm-title {
    font-family: 'Arvo', Georgia, Utopia, Palatino, 'Palatino Linotype', serif;
    font-weight: 700;
    font-size: 1.2em;
    color: #f48131;
    text-shadow: 0 0 2px #fff;
}
/* Loading */
#detourmap .loading {
    font-family: monospace, sans-serif;
    width: 400px;
    height: 120px;
    padding: 8px;
    text-align: center;
    border: 6px solid #eaeaea;
    background-color: #fbf9f0;
    color: #bababa;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -214px;
    margin-top: -70px;
    border-radius: 12px;
}
#detourmap .loading h3 {
    font-family: monospace, sans-serif;
    font-size: 2em;
    line-height: 1em;
    margin: 0;
}
#detourmap .loading span {
    font-size: 1.4em;
    display: block;
}

/*ToolBar*/
#user_menu_login{
    position: absolute;
    top:0;
    left: 0;
    border: 1px #bababa solid;
    box-shadow: 0 4px 12px 1px rgba(0, 0, 0, 0.35);
    background:  #f5f6f7 url('../img/fondoDetour.jpg');
}
.user_form{
    float: right;
}

/* Communities HOME PAGE */
#communityHome-page #detourmap { margin-top: 0; }
#communityHome-page .lbar {
    padding: 0 26px;
    color: white;
    background-color: #40607F;
    border-color: #40607F;
    -webkit-border-radius: 0;
    border-radius: 0;
}
#communityHome-page .lbar h2 {
    font-family: 'Arvo', Georgia, Utopia, Palatino, 'Palatino Linotype', serif;
    font-weight: 700;
    font-size: 1.7em;
    line-height: 1.2em;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.75);
    margin: 0.8em 0 0.5em;
}
#communityHome-page .lbar h3 {
    font-family: 'Arvo', Georgia, Utopia, Palatino, 'Palatino Linotype', serif;
    font-weight: 700;
    font-size: 1.2em;
    line-height: 1.1em;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.75);
    margin: 0.7em 0 0.4em;
}
#communityHome-page .lbar p {
    line-height: 1.25em;
}
#communityHome-page .lbar a {
    color: #addff2;
    text-decoration: underline;
}
#communityHome-page .lbar button {
    font-weight: 700;
    width: 100%;
}
#communityHome-page .lbar .line {
    height: 10px;
    border-bottom: 1px solid #fff;
    text-align: center;
}
#communityHome-page .lbar .line img {
    padding: 0 16px;
    margin: 0;
    background-color: #40607F;
}
/* Categories */
#categories {
    height: 53px;
    overflow: hidden;
    border: 1px solid #f48131;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
}
#categories ul {
    display: table;
    width: 100%;
    table-layout: fixed;
    border-collapse: separate;
    background: #f48131;
    list-style: none;
}
.ie #categories ul {
    border-spacing: 1px 0;
}
#categories ul li {
    display: table-cell;
    color: #fff;
    width: 100%;
    overflow: hidden;
    border-right: 1px solid #f48131;
    border-left: 1px solid #ffaa33;
    background: #f48131;
    background: -moz-linear-gradient(top, #ffaa33 0%, #ff8833 100%);
    background: -webkit-linear-gradient(top, #ffaa33 0%,#ff8833 100%);
    background: -o-linear-gradient(top, #ffaa33 0%,#ff8833 100%);
    background: -ms-linear-gradient(top, #ffaa33 0%,#ff8833 100%);

}
.ie #categories ul li{
    width: auto;

    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffaa33', endColorstr='#ff8833',GradientType=0 );
}
@media only screen and (-o-min-device-pixel-ratio : 1/1) {
    #categories ul li { width:auto; }
}
#categories ul li:hover {
    background: #ff8833;
    filter: none;
}
#categories ul li:active {
    background: #ff8833;
    box-shadow: inset 0 0 8px rgba(0,0,0,0.4);
    border-left: 1px solid #ba6224;
    filter: none;
}
#categories ul li.selected {
    background: #f48131;
    border-left: 1px solid #ba6224;
    box-shadow: inset 0 0 12px rgba(0,0,0,0.5);
    filter: none;
}
#categories ul li:first-child {
    border-left: none;
    -webkit-border-top-left-radius: 6px;
    -webkit-border-bottom-left-radius: 6px;
    -moz-border-radius-topleft: 6px;
    -moz-border-radius-bottomleft: 6px;
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;
}
#categories ul li:last-child {
    border-right: none;
    -webkit-border-top-right-radius: 6px;
    -webkit-border-bottom-right-radius: 6px;
    -moz-border-radius-topright: 6px;
    -moz-border-radius-bottomright: 6px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
}
#categories .cat-title {
    width: auto;
    padding: 4px;
    text-align: center;
    line-height: 0.7em;
}
#categories .cat-title span {
    display: block;
    width: auto;
    margin: 0;
    cursor: default;
}
#categories .cat-title .img {
    display: inline-block;
    height: 30px;
    width: 30px;
    background-position: 0 -36px;
}
#categories .cat-title span.title {
    font-family: 'Istok Web', sans-serif;
    font-size: 1em;
    font-weight: 400;
    line-height: 1.2em;
}
#categories .cat-content {
    display: none;
    overflow: hidden;
    height: 0;
}

/* Panel Lateral */
.lbar .welcome {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    position: absolute;
    width: 100%;
    height: 0;
    top: 0;
    left: 0;
    opacity: 0;
    filter: alpha(opacity = 0);
    -moz-transition: all 0.33s ease-out;
    -o-transition: all 0.33s ease-out;
    -webkit-transition: all 0.33s ease-out 0s;
    -ms-transition: all 0.33s ease-out;
    transition: all 0.33s ease-out;
    overflow: hidden;
}
.lbar.clear .welcome {
    opacity: 1;
    filter: alpha(opacity = 1);
    height: 100%;
}
.lbar .welcome img {
    max-width: 100%;
}
.lbar .welcome .ways {
    margin-top: 6px;
}
.lbar .welcome ul {
    display: block;
    list-style: none;
    padding: 35px 20px 20px;
}
.lbar .welcome ul li {
    position: relative;
    margin-bottom: 14px;
}
.lbar .welcome ul li strong {
    display: block;
    color: #fff;
    font-family: 'Arvo', Georgia, Utopia, Palatino, 'Palatino Linotype', serif;
    font-weight: 400;
    font-size: 1.5em;
    line-height: 1;
}
.lbar .welcome ul li.or {
    display: block;
    text-align: center;
